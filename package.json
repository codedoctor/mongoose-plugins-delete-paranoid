{
  "author": "Martin Wawrusch <martin@wawrusch.com> (http://martinatsunset.com)",
  "name": "mongoose-plugins-delete-paranoid",
  "description": "Adds paranoid deletion to a mongoose schema",
  "version": "0.2.0",
  "main" : "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/codedoctor/mongoose-plugins-delete-paranoid.git"
  },
  "bin" : {
  },
  "directories" : {
    "lib" : "./lib",
    "test" : "./test"
  },
